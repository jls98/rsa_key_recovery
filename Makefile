N = 

all: task

task: venv/bin/factorgpg gnupg.txt
	venv/bin/factorgpg gnupg.txt $(N)

venv/bin/factorgpg: venv/
	venv/bin/pip install -e .

venv/:
	python3 -m venv venv
	venv/bin/pip install --upgrade pip

clean:
	rm -rf build/
	rm -rf src/factorgpg/__pycache__
	rm -rf src/*.egg-info
	rm -rf venv/

.PHONY: all task clean
